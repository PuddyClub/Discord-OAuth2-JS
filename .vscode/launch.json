    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\test\\index.js"
        },
        {
            "command": "git push -u origin --all",
            "name": "Github Push",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "npm publish --access public",
            "name": "NPM Publish",
            "request": "launch",
            "type": "node-terminal"
        }
    ]
}